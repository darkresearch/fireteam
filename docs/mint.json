{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Fireteam",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#FF4500",
    "light": "#FF6347",
    "dark": "#DC143C",
    "background": {
      "dark": "#0D1117"
    },
    "anchors": {
      "from": "#FF4500",
